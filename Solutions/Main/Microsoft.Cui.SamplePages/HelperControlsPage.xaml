    xmlns:local="clr-namespace:Microsoft.Cui.SamplePages;assembly=Microsoft.Cui.SamplePages"
    Width="Auto" Height="Auto">
    <Grid x:Name="LayoutRoot">
        <StackPanel>
            <local:DecoratorTextBoxPage  Margin="5,5,5,2.5"/>
            <local:EncodableInputBoxPage Margin="5,5,5,2.5"/>
            <local:IndentedLabelPage Margin="5,5,5,2.5"/>
            <local:LevelOfDetailPage Margin="5,5,5,2.5"/>
            <local:PanelViewerPage Margin="5,5,5,2.5"/>
